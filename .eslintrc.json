{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-unused-vars": "warn",
    "indent": ["error", "tab"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-var": ["error"],
    "eol-last": "error"
  }
}